<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nsn.dubbo.dubboinvoker.dal.dao.ConfigDao">

    <select id="selectAllRepository" resultMap="com.nsn.dubbo.dubboinvoker.dal.mapper.ConfigMapper.BaseResultMap">
        select * from CONFIG where KEY = 'nexus-repository'
    </select>

    <select id="selectAllDependency" resultMap="com.nsn.dubbo.dubboinvoker.dal.mapper.ConfigMapper.BaseResultMap">
        select * from CONFIG where KEY = 'maven-dependency'
    </select>
</mapper>